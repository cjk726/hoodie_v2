<div class="page-header">
  <h3>Outfit Item #<%= @outfit_item.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Outfit</dt>
      <dd><%= @outfit_item.outfit_id %></dd>

      <dt>Clothing item</dt>
      <dd><%= @outfit_item.clothing_item_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/outfit_items" class="btn btn-primary">
        Back
      </a>
      <a href="/outfit_items/<%= @outfit_item.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_outfit_item/<%= @outfit_item.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
